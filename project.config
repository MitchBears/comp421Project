[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Mitchell Bears (MitchBears)
[access "refs/for/refs/*"]
	read = group user/Mitchell Bears (MitchBears)
	push = group user/Mitchell Bears (MitchBears)
	label-Code-Review = -2..+2 group user/Mitchell Bears (MitchBears)
	label-Verified = -1..+1 group user/Mitchell Bears (MitchBears)
	rebase = group user/Mitchell Bears (MitchBears)
	removeReviewer = group user/Mitchell Bears (MitchBears)
	submit = group user/Mitchell Bears (MitchBears)
[access "refs/heads/*"]
	read = group user/Mitchell Bears (MitchBears)
	create = group user/Mitchell Bears (MitchBears)
	push = +force group user/Mitchell Bears (MitchBears)
	pushMerge = group user/Mitchell Bears (MitchBears)
[access "refs/tags/*"]
	push = group user/Mitchell Bears (MitchBears)
	push = +force group user/Mitchell Bears (MitchBears)
